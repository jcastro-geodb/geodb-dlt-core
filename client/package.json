    "electron-context-menu": "^0.12.1",
    "electron-is-dev": "^1.1.0",
    "ensure-array": "^1.0.0",
    "formik": "^1.5.7",
    "fs-extra": "^7.0.1",
    "jquery": "^3.4.0",
    "js-yaml": "^3.13.1",
    "json2yaml": "^1.1.0",
    "nedb": "^1.8.0",
